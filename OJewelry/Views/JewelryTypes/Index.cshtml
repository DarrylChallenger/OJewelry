@model IEnumerable<OJewelry.Models.JewelryType>

@{
    ViewBag.Title = "Index";
}

<h2>Jewelry Types</h2>

<p>
    @{
        int? vbid = ViewBag.Id;
        <text>@Html.ActionLink("Create New", "Create")</text>
    }

</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.Id }) 
        </td>
    </tr>
}

</table>
<div class="row">
    <div class="col-sm-4">
        @Html.ActionLink("Manage Companies", "Index", "Companies")
    </div>
</div>
