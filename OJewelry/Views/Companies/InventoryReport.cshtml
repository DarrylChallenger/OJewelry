@model IEnumerable<OJewelry.Models.InventoryReportModel>

@{
    ViewBag.Title = "InventoryReport";
}

<h2>InventoryReport</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.PresenterName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PresenterPhone)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PresenterEmail)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Quantity)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ReturnQty)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.PresenterName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PresenterPhone)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PresenterEmail)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Quantity)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Notes)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ReturnQty)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
